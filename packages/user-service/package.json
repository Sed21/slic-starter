{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User Service for SLIC Starter",
  "scripts": {
    "start": "source ../localstack.env && sls offline start --region us-east-1 --migrate true",
    "lint": "eslint .",
    "format": "eslint --cache --fix \"**/*.js\"",
    "coverage": "npm run test -- --cov --coverage-report=html",
    "test": "SLIC_STAGE=test AWS_XRAY_CONTEXT_MISSING=LOG_ERROR AWS_XRAY_LOG_LEVEL=silent tap \"test/**/*.test.js\" --branches=100 --statements=100 --functions=100 --lines=100"
  },
  "author": "Eoin Shanaghy <eoin.shanaghy@fourtheorem.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fourTheorem/slic-starter"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "aws-sdk": "^2.1153.0",
    "aws-sdk-mock": "^5.7.0",
    "awscred": "^1.5.0",
    "mock-amplify-auth": "^2.2.0",
    "proxyquire": "^2.1.3",
    "serverless-bundle": "^5.3.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.8.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-slic-watch-plugin": "^1.2.0"
  },
  "dependencies": {
    "aws-xray-sdk": "^3.3.6",
    "slic-tools": "*"
  },
  "nyc": {
    "exclude": [
      "**/test/**/*.js"
    ]
  }
}
